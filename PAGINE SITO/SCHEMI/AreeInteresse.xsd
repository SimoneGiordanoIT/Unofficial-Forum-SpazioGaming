<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

<xsd:element name="aree_int">
   <xsd:complexType> 
      <xsd:sequence>
	 <xsd:element ref="gioco" maxOccurs="unbounded" />
      </xsd:sequence>
   </xsd:complexType>
</xsd:element>

<xsd:element name="gioco">
   <xsd:complexType>
      <xsd:sequence>
         <xsd:element ref="ID" />
         <xsd:element ref="nome" />
         <xsd:element ref="tag" />
         <xsd:element ref="flag_attivo" />
      </xsd:sequence>
   </xsd:complexType>
</xsd:element>

<xsd:element name="ID" type="xsd:int" />

<xsd:element name="nome" type="xsd:string" />

<xsd:element name="tag" type="xsd:string"/>

<xsd:element name="flag_attivo" type="xsd:boolean"/>


</xsd:schema>